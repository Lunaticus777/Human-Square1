<script>
    function showSection(id) {
        // Cacher toutes les sections
        document.querySelectorAll('.service-section').forEach(function (el) {
            el.classList.add('d-none');
            el.classList.remove('d-block');
        });

        // Afficher celle demandée
        const section = document.getElementById(id);
        if (section) {
            section.classList.remove('d-none');
            section.classList.add('d-block');
            
        }

        // Mettre en surbrillance le bon bouton
        document.querySelectorAll(".sidebar-fixed button").forEach(function (btn) {
            btn.classList.remove("text-primary", "fw-bold");
        });

        const activeBtn = document.querySelector(`.sidebar-fixed button[onclick="showSection('${id}')"]`);
        if (activeBtn) {
            activeBtn.classList.add("text-primary", "fw-bold");
        }
    }

    document.addEventListener("DOMContentLoaded", function () {
        showSection('coaching');
    });
</script>







